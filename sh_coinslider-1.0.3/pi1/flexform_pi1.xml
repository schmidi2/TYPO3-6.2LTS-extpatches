<?xml version="1.0" encoding="utf-8"?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<tab1>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:sh_coinslider/pi1/locallang.xml:configuration</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<effect> // Selectbox
						<TCEforms>
							<label>LLL:EXT:sh_coinslider/pi1/locallang.xml:effect</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">Random</numIndex>
										<numIndex index="1">random</numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">Swirl</numIndex>
										<numIndex index="1">swirl</numIndex>
									</numIndex>
									<numIndex index="2">
										<numIndex index="0">Rain</numIndex>
										<numIndex index="1">rain</numIndex>
									</numIndex>
									<numIndex index="3">
										<numIndex index="0">Straight</numIndex>
										<numIndex index="1">straight</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<multiple>0</multiple>
								<selectedListStyle>width:150px</selectedListStyle>
								<itemListStyle>width:150px</itemListStyle>
							</config>
						</TCEforms>
					</effect>
					<navibuttons> // Selectbox
						<TCEforms>
							<label>LLL:EXT:sh_coinslider/pi1/locallang.xml:navibuttons</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">LLL:EXT:sh_coinslider/pi1/locallang.xml:hide</numIndex>
										<numIndex index="1">false</numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">LLL:EXT:sh_coinslider/pi1/locallang.xml:show</numIndex>
										<numIndex index="1">true</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<multiple>0</multiple>
								<selectedListStyle>width:150px</selectedListStyle>
								<itemListStyle>width:150px</itemListStyle>
							</config>
						</TCEforms>
					</navibuttons>
					<sliderwidth>  // Standard Input
						<TCEforms>
							<label>LLL:EXT:sh_coinslider/pi1/locallang.xml:sliderwidth</label>
							<config>
								<type>input</type>
								<eval>text</eval>
								<size>3</size>
								<max>3</max>
							</config>
						</TCEforms>
					</sliderwidth>
					<sliderheight>  // Standard Input
						<TCEforms>
							<label>LLL:EXT:sh_coinslider/pi1/locallang.xml:sliderheight</label>
							<config>
								<type>input</type>
								<eval>text</eval>
								<size>3</size>
								<max>3</max>
							</config>
						</TCEforms>
					</sliderheight>
					<squeresperwidth>  // Standard Input
						<TCEforms>
							<label>LLL:EXT:sh_coinslider/pi1/locallang.xml:squeresperwidth</label>
							<config>
								<type>input</type>
								<eval>text</eval>
								<size>2</size>
								<max>2</max>
							</config>
						</TCEforms>
					</squeresperwidth>
					<squeresperheight>  // Standard Input
						<TCEforms>
							<label>LLL:EXT:sh_coinslider/pi1/locallang.xml:squaresperheight</label>
							<config>
								<type>input</type>
								<eval>text</eval>
								<size>2</size>
								<max>2</max>
							</config>
						</TCEforms>
					</squeresperheight>
					<delayimages>  // Standard Input
						<TCEforms>
							<label>LLL:EXT:sh_coinslider/pi1/locallang.xml:delay.images</label>
							<config>
								<type>input</type>
								<eval>text</eval>
								<size>4</size>
								<max>4</max>
							</config>
						</TCEforms>
					</delayimages>
					<delaysquares>  // Standard Input
						<TCEforms>
							<label>LLL:EXT:sh_coinslider/pi1/locallang.xml:delay.squares</label>
							<config>
								<type>input</type>
								<eval>text</eval>
								<size>2</size>
								<max>2</max>
							</config>
						</TCEforms>
					</delaysquares>
					<imagerecords>  // Auswahlliste
						<TCEforms>
							<label>LLL:EXT:sh_coinslider/pi1/locallang.xml:imagerecords</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<multiple>1</multiple>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<size>4</size>
							</config>
						</TCEforms>
					</imagerecords>
				</el>
			</ROOT>
		</tab1>
	</sheets>
</T3DataStructure>
